{
  "metadata": {
    "title": "Michael Pereira | monkeycycle",
    "description": ""
  },
  "source": "app",
  "destination": "dist",
  "plugins": {
    "metalsmith-ignore": [
      "**/.DS_Store",
      "unfinished/*",
      "javascripts/**.*"
    ],
    "metalsmith-metallic": true,
    "metalsmith-markdown": {},
    "metalsmith-permalinks": {
      "pattern": ":permalink"
    },
    "metalsmith-templates": {
      "engine": "handlebars"
    },
    "metalsmith-sitemap":{
      "output": "sitemap.xml",
      "hostname": "http://monkeycycle.org"
    }
  }
}
